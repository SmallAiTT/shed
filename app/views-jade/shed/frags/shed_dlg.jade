-var pvl = pvlDs.pvlMap["shed_shed_mgr_c"]
if pvl
    -var dlgId = 'dlg_' + pvl.code
    -var btnsId = '#btns_' + pvl.code
    .easyui-dialog(id="#{dlgId}", title="#{pvl.name}", data-options="width:600,height:400,modal:true,closed:true,buttons:'#{btnsId}'")
        div
            form(method="post")
                table
                    tr
                        mixin validBox_td("code", "零件编号：", "required:true")
                    tr
                        mixin validBox_td("name", "零件名称：", "required:true")
                    tr
                        mixin validBox_td("isBase", "是否是基础零件：", "required:true")
                    tr
                        mixin validBox_td("img", "零件图示：", "")
                    tr
                        mixin validBox_td("spec", "规格：", "")
                    tr
                        mixin validBox_td("mates", "材质：", "")
                    tr
                        mixin validBox_td("unit", "单位：", "")
                    tr
                        mixin textarea_td("remark", "备注：", "", 50)
                //table end
             //form end

    mixin dlgBtns(pvl)
    //btns end
//shed_shed_mgr_c end

